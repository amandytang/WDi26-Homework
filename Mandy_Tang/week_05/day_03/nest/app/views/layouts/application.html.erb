<!DOCTYPE html>
<html>
  <head>
    <title>Nest</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="container">

    <nav>
      <%= link_to 'All Birds', birds_path %>
      <%= link_to 'New Bird', new_bird_path %>
      <%= link_to 'All Eggs', eggs_path %>
      <%= link_to 'New Egg', new_egg_path %>

    </nav>
    <%= yield %>
    </div>
  </body>
</html>
